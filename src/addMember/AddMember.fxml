                  <Label layoutX="32.0" layoutY="25.0" text="Add Member" />
               </children>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </Pane>
            <JFXTextField fx:id="txtname" labelFloat="true" promptText="Name ">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtnic" labelFloat="true" promptText="N.I.C. Number">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtemp" labelFloat="true" promptText="Employeement">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtcontact" labelFloat="true" promptText="Contact Number">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtemail" labelFloat="true" promptText="Email ">
