            <JFXButton fx:id="btnadd" onAction="#addMember" prefHeight="58.0" prefWidth="298.0" style="-fx-background-color: #001575;" text="Add" textFill="WHITE" />
            <JFXButton fx:id="btncancel" layoutX="10.0" layoutY="10.0" prefHeight="58.0" prefWidth="298.0" style="-fx-border-color: #001575;" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
         </padding>
      </HBox>
   </children>
